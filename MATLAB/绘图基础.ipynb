   "source": [
    "clear \n",
    "clf % 重置当前图形窗口为初始状态\n",
    "y1 = [15, 35, 10, 20, 20];\n",
    "y2 = [15, 35, 10; 20, 20, 15; 10, 15, 30];\n",
    "subplot(2, 2, 1)\n",
    "bar(y1, 'c')\n",
    "subplot(2, 2, 2)\n",
    "bar(y2, 'grouped') % 分组式条形图\n",
    "subplot(2, 2, 3)\n",
    "barh(y2, 'stacked') % 水平放置累积条形图\n",
    "subplot(2, 2, 4)\n",
    "bar3(y2, 'grouped') % 三维立体条形图"
   ]
